@model Quick5.Models.Garantie

<table class="table table-condensed table-striped table-hover">
<thead>
    <tr>
        <th>
            @Html.ActionLink(" ", "Details", "Garanties", new { id = Model.Garantie_ID}, new { @class="glyphicon glyphicon-play" })
            Garantie
        </th>
        <th>Date</th>
        <th>Option</th>
        <th>Montant</th>
        <th>Début</th>
        <th>Fin</th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>Principale</td>
        <td>@Html.DisplayFor(m => m.GarDate)</td>
        <td>@Html.DisplayFor(m => m.GarOption)</td>
        <td>@Html.DisplayFor(m => m.GarMontant)</td>
        <td>@Html.DisplayFor(m => m.GarDebut)</td>
        <td>@Html.DisplayFor(m => m.GarFin)</td>
    </tr>
    <tr>
        <td>Complémentaire</td>
        <td></td>
        <td></td>
        <td>@Html.DisplayFor(m => m.CplMontant)</td>
        <td>@Html.DisplayFor(m => m.CplDebut)</td>
        <td>@Html.DisplayFor(m => m.CplFin)</td>
    </tr>
    <tr>
        <td>Interne</td>
        <td></td>
        <td></td>
        <td>@Html.DisplayFor(m => m.IntMontant)</td>
        <td>@Html.DisplayFor(m => m.IntDebut)</td>
        <td>@Html.DisplayFor(m => m.IntFin)</td>
    </tr>
    <tr>
        <td>OAL</td>
        <td></td>
        <td></td>
        <td>@Html.DisplayFor(m => m.OalMontant)</td>
        <td>@Html.DisplayFor(m => m.OalDebut)</td>
        <td>@Html.DisplayFor(m => m.OalFin)</td>
    </tr>
    <tr>
        <td>CAP</td>
        <td></td>
        <td>@Html.DisplayFor(m => m.CapOption)</td>
        <td>@Html.DisplayFor(m => m.CapMontant)</td>
        <td>@Html.DisplayFor(m => m.CapDebut)</td>
        <td>@Html.DisplayFor(m => m.CplFin)</td>
    </tr>
    <tr>
        <td>Déblocage</td>
        <td></td>
        <td></td>
        <td>@Html.DisplayFor(m => m.Deblocage)</td>
        <td></td>
        <td></td>
    </tr>
</tbody>
<tfoot>
    <tr>
        <th>Total</th>
        <th></th>
        <th></th>
        <th>@Html.DisplayFor(m => m.Totale)</th>
        <th></th>
        <th></th>
    </tr>
</tfoot>
</table>
