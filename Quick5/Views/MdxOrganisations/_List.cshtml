@model IEnumerable<Quick5.Models.MdxOrganisation>

<table class="table table-condensed table-striped table-hover">
<thead>
    <tr>
        <th class="hidden-print"></th>
        <th>ID</th>
        <th>Libellé</th>
        <th>Parent_ID</th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model)
{
    <tr>
        <th class="hidden-print iconwidth">@Html.ActionLink(" ", "Details", "MdxOrganisations", new { id = item.ID }, new { @class="glyphicon glyphicon-play" })</th>
        <td>@Html.DisplayFor(modelItem => item.ID)</td>
        <td>@Html.DisplayFor(modelItem => item.Libelle)</td>
        <td>@Html.DisplayFor(modelItem => item.Parent_ID)</td>
    </tr>
}
</tbody>
</table>
